 methods: {\n      selectOption(option) {\n        this.$emit('select', option)\n        this.areOptionsVisible = false;\n      },\n      hideSelect() {\n        this.areOptionsVisible = false;\n      }\n\n    },\n    mounted() {\n      document.addEventListener('click', this.hideSelect.bind(this), true)\n    },\n    beforeDestroy() {\n      document.removeEventListener('click', this.hideSelect)\n    }\n  }\n</script>\n\n<style>\n  .v-select {\n    position: relative;\n    width: 200px;\n    cursor: pointer;\n    text-align: left;\n  }\n  .title {\n    border: solid 1px #aeaeae;\n    padding: 8px;\n  }\n  .v-select p {\n    margin: 0;\n  }\n\n  .options {\n    border: solid 1px #aeaeae;\n    background: #ffffff;\n    position: absolute;\n    top: 30px;\n    left: 0;\n    width: 100%;\n    padding: 8px;\n  }\n\n  .options p:hover {\n    background: #e7e7e7;\n  }\n</style>"]}]}